<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
		<servlet-name>servletConso</servlet-name>
		<servlet-class>fr.istic.gaegwt.server.Conso</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>servletConso</servlet-name>
		<url-pattern>/gaegwt/consommation</url-pattern>
	</servlet-mapping>
	
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>GaeGwt.html</welcome-file>
  </welcome-file-list>

</web-app>
